    "start:compile:watch": "concurrently -n \"compile,run\" \"yarn run compile:watch\" \"yarn start\""
  },
  "devDependencies": {
    "@types/argv": "^0.0.4",
    "@types/node": "^10.9.4",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4",
    "ts-loader": "^5.1.0",
    "typescript": "^3.0.3",
    "webpack": "^4.18.1",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "argv": "^0.0.2"
  }
}
